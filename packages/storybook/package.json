{
  "name": "@react-changelog/storybook",
  "version": "0.0.1",
  "description": "A changelog modal made in React",
  "main": "index.js",
  "scripts": {
    "prestart": "yarn install && lerna run --scope @react-changelog/modal build",
    "start": "concurrently \"yarn serve\" \"yarn storybook\"",
    "lint": "eslint --ext .jsx,.js stories",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "serve": "cd ./stories && serve"
  },
  "author": "FRIN Yvonnick <yvonnick.frin@zenika.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "8.2.2",
    "concurrently": "3.5.1",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "html-loader": "0.5.5",
    "http-proxy-middleware": "0.17.4",
    "lerna": "2.9.0",
    "markdown-loader": "2.0.2",
    "serve": "6.5.2"
  },
  "dependencies": {
    "@react-changelog/modal": "0.0.1",
    "@storybook/react": "3.3.15",
    "babel-polyfill": "6.26.0",
    "front-matter": "2.3.0",
    "lodash": "4.17.5",
    "prop-types": "15.6.1",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-markdown": "3.3.0",
    "react-modal": "3.3.1",
    "semver-compare": "1.0.0"
  }
}
